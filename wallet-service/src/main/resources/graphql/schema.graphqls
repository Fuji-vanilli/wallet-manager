type Query {
    userWallets: [Wallet],
    walletById(id: String): Wallet
}

type Wallet {
    id: String,
    amount: Float,
    createdAt: Float,
    currency: Currency,
    walletTransactions: [WalletTransaction]
}

type Currency {
    code: String,
    name: String,
    symbol: String,
    salePrice: Float,
    purchasePrice: Float
}

type WalletTransaction {
    id: Float,
    amount: Float,
    timestamp: Float,
    type: String
}